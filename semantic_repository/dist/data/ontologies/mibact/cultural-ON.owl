    <!-- http://dati.beniculturali.it/cis/Offer -->

    <owl:Class rdf:about="http://dati.beniculturali.it/cis/Offer">
        <rdfs:label xml:lang="en">Offer</rdfs:label>
        <rdfs:label xml:lang="it">Offerta</rdfs:label>
