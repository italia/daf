<http://dati.gov.it/onto/iotapit/> rdf:type owl:Ontology ;
                                   
                                   owl:versionInfo "Versione 0.5 - 02 Luglio 2017"^^xsd:dateTime ;
                                   
                                   terms:issued "2017-05-15"@en ;
                                   
                                   dc:creator "Agency for Digital Italy - AgID"@en ,
                                              "Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)"@en ,
                                              "Italian Digital Transformation Team"@en ;
                                   
                                   rdfs:comment "This is the ontology used to represent IoT events. It can be used to model temporal series and measures of sensors of different type."@en ;
                                   
                                   owl:versionInfo "Version 0.5 - July 03 2017"@en ;
                                   
                                   dc:creator "Agenzia per l'Italia Digitale - AgID"@it ,
                                              "Istituto di Scienze e Tecnologie della Cognizione del CNR - Semantic Technology Lab (STLab)"@it ;
                                   
                                   rdfs:comment "Questo è il profilo applicativo Questa è l'ontologia di eventi IoT. Essa può essere utilizzata per esempio per rappresentare serie temporali e misure di sensoristica di vario tipo."@it ;
                                   
                                   dc:creator "Team per la Trasformazione Digitale"@it ;
                                   
                                   rdfs:isDefinedBy <http://dati.gov.it/onto/iotap> ;
                                   
                                   prov:wasDerivedFrom <http://dati.gov.it/onto/iotapit/0.4> ;
                                   
                                   owl:versionIRI <http://dati.gov.it/onto/iotapit/0.5> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://dati.gov.it/onto/iotapit/date

:date rdf:type owl:AnnotationProperty ;
      
      rdfs:domain :TimeInterval .



###  http://dati.gov.it/onto/iotapit/endDate

:endDate rdf:type owl:AnnotationProperty ;
         
         rdfs:domain :TimeInterval .



###  http://dati.gov.it/onto/iotapit/startDate

:startDate rdf:type owl:AnnotationProperty ;
           
           rdfs:domain :TimeInterval .



###  http://purl.org/dc/elements/1.1/creator

dc:creator rdf:type owl:AnnotationProperty .



###  http://www.w3.org/ns/prov#wasDerivedFrom

prov:wasDerivedFrom rdf:type owl:AnnotationProperty .





#################################################################
#
#    Datatypes
#
#################################################################


###  http://dati.gov.it/onto/iotapit/TimeInterval

:TimeInterval rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://dati.gov.it/onto/iotapit/atTime

:atTime rdf:type owl:ObjectProperty ;
        
        rdfs:label "at time"@en ,
                   "al tempo"@it ;
        
        rdfs:comment "Property that links an observation to the time interval in which is performed."@en ;
        
        owl:versionInfo "stable"@en ;
        
        rdfs:comment "Proprietà che lega un'osservazione all'intervallo temporale in cui è condotta."@it ;
        
        owl:versionInfo "stabile"@it ;
        
        rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
        
        rdfs:domain :Observation ;
        
        rdfs:range :TimeInterval .



###  http://dati.gov.it/onto/iotapit/consistsOf

:consistsOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "consists of"@en ,
                       "consiste di"@it ;
            
            rdfs:comment "Property that links a collection of observations with its observations."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Proprietà che collega una collezione di osservazioni con le sue osservazioni."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
            
            rdfs:range :Observation ;
            
            rdfs:domain :ObservationCollection .



###  http://dati.gov.it/onto/iotapit/generationTime

:generationTime rdf:type owl:ObjectProperty ;
                
                rdfs:domain :Observation ;
                
                rdfs:range :TimeInterval ;
                
                rdfs:subPropertyOf :atTime .



###  http://dati.gov.it/onto/iotapit/hasAssociatedGeometry

:hasAssociatedGeometry rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "has associated geometry"@en ,
                                  "ha geometria associata"@it ;
                       
                       rdfs:comment "The property links the Observation to its geometry that is described by a type (point, line, polygon, etc.), coordinates and a coordinate reference system."@en ;
                       
                       owl:versionInfo "stable"@en ;
                       
                       rdfs:comment "Proprietà che lega un'osservazione alla sua geometria descritta in termini di tipo (linea, punto, poligono, ecc.), coordinate geografiche e sistema di riferimento delle coordinate."@it ;
                       
                       owl:versionInfo "stabile"@it ;
                       
                       rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                       
                       rdfs:range :Geometry ;
                       
                       rdfs:domain :Observation .



###  http://dati.gov.it/onto/iotapit/hasFeatureOfInterest

:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has feature of interest"@en ,
                                 "ha caratteristica di interesse"@it ;
                      
                      rdfs:comment "Property that links the Observation to a feature of interest."@en ;
                      
                      owl:versionInfo "stable"@en ;
                      
                      rdfs:comment "Proprietà che lega un'osservazione a una caratteristica di interesse."@it ;
                      
                      owl:versionInfo "stabile"@it ;
                      
                      rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                      
                      rdfs:range :FeatureOfInterest ;
                      
                      rdfs:domain :Observation .



###  http://dati.gov.it/onto/iotapit/hasMeasurementQualityValue

:hasMeasurementQualityValue rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "has measurement quality value"@en ,
                                       "ha valore di qualità di misurazione"@it ;
                            
                            rdfs:comment "Property that links the Measrurement Quality with its value."@en ;
                            
                            owl:versionInfo "stable"@en ;
                            
                            rdfs:comment "Proprietà che collega la Misurazione di qualità con il relativo valore."@it ;
                            
                            owl:versionInfo "stabile"@it ;
                            
                            rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                            
                            rdfs:domain :MeasurementQuality ;
                            
                            rdfs:range :MeasurementQualityValue ;
                            
                            owl:inverseOf :isMeasurementQualityValueOf .



###  http://dati.gov.it/onto/iotapit/hasMeasurementUnit

:hasMeasurementUnit rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "has measurement unit"@en ,
                               "ha unità di misura"@it ;
                    
                    rdfs:comment "Property that links an ObservedValue or a MesurementCapability to its Measurement Unit"@en ;
                    
                    owl:versionInfo "stable"@en ;
                    
                    rdfs:comment "Proprietà che lega un valore osservato o un parametro di misurazione con la sua unità di misura."@it ;
                    
                    owl:versionInfo "stabile"@it ;
                    
                    rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                    
                    rdfs:range :MeasurementUnit ;
                    
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :MeasurementQuality
                                                :ObservationValue
                                              )
                                ] .



###  http://dati.gov.it/onto/iotapit/hasObservationParameter

:hasObservationParameter rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "has observation parameter"@en ,
                                    "ha parametro dell'osservazione"@it ;
                         
                         rdfs:comment "Property that links an observation to its parameter being observed."@en ;
                         
                         owl:versionInfo "stable"@en ;
                         
                         rdfs:comment "Proprietà che collega un'osservazione al suo parametro osservato."@it ;
                         
                         owl:versionInfo "stabile"@it ;
                         
                         rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                         
                         rdfs:domain :Observation ;
                         
                         rdfs:range :ObservationParameter .



###  http://dati.gov.it/onto/iotapit/hasObservationValue

:hasObservationValue rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has observation value"@en ,
                                "ha valore dell'osservazione"@it ;
                     
                     rdfs:comment "Property that links an Observation to its value."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Proprietà che lega un'osservazione al suo valore."@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                     
                     rdfs:domain :Observation ;
                     
                     rdfs:range :ObservationValue .



###  http://dati.gov.it/onto/iotapit/hasQuality

:hasQuality rdf:type owl:ObjectProperty ;
            
            rdfs:label "has quality"@en ,
                       "ha qualità"@it ;
            
            rdfs:comment "Property that links an Observation to a number of measurement capabilities."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Proprietà che lega un'osservazione a una serie di parametri di qualità."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
            
            rdfs:range :MeasurementQuality ;
            
            rdfs:domain :Observation .



###  http://dati.gov.it/onto/iotapit/hasValueForObservationParameter

:hasValueForObservationParameter rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:domain :ObservationParameter ;
                                 
                                 rdfs:range :ObservationValue .



###  http://dati.gov.it/onto/iotapit/isFeatureOfInterestFor

:isFeatureOfInterestFor rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "is feature of interest for"@en ,
                                   "è caratteristica di interesse per"@it ;
                        
                        rdfs:comment "Inverse property of has feature of interest that links a feature of interest to an observation."@en ;
                        
                        owl:versionInfo "stable"@en ;
                        
                        rdfs:comment "Proprietà inversa di ha caratteristica di interesse che lega una caratteristica di interesse a un'osservaizione."@it ;
                        
                        owl:versionInfo "stabile"@it ;
                        
                        rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                        
                        rdfs:domain :FeatureOfInterest ;
                        
                        rdfs:range :Observation ;
                        
                        owl:inverseOf :hasFeatureOfInterest .



###  http://dati.gov.it/onto/iotapit/isMeasurementQualityValueOf

:isMeasurementQualityValueOf rdf:type owl:ObjectProperty ;
                             
                             rdfs:label "is measurement quality value of"@en ,
                                        "è valore di qualità di misurazione di"@it ;
                             
                             rdfs:comment "Inverse property of has measurement quality value that links the measurement quality value to the measurment quality."@en ;
                             
                             owl:versionInfo "stable"@en ;
                             
                             rdfs:comment "Proprietà inversa che lega un valore della qualità di misurazione con la qualità di misurazione."@it ;
                             
                             owl:versionInfo "stabile"@it ;
                             
                             rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                             
                             rdfs:range :MeasurementQuality ;
                             
                             rdfs:domain :MeasurementQualityValue .



###  http://dati.gov.it/onto/iotapit/isObservationParameterFor

:isObservationParameterFor rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "is observation parameter for"@en ,
                                      "è parametro dell'osservazione per"@it ;
                           
                           rdfs:comment "Inverse property of hasObservationParameter that links a parameter to its related observation."@en ;
                           
                           owl:versionInfo "stable"@en ;
                           
                           rdfs:comment "Proprietà inversa di ha attributo osservato che lega un attributo osservato alla sua relativa osservazione."@it ;
                           
                           owl:versionInfo "stabile"@it ;
                           
                           rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                           
                           rdfs:range :Observation ;
                           
                           rdfs:domain :ObservationParameter ;
                           
                           owl:inverseOf :hasObservationParameter .



###  http://dati.gov.it/onto/iotapit/isObservationValueOf

:isObservationValueOf rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is osbservation value of"@en ,
                                 "è valore osservato di"@it ;
                      
                      rdfs:comment "Inverse property of hasObservedValue that links an ObservedValue to its related Observation."@en ;
                      
                      owl:versionInfo "stable"@en ;
                      
                      rdfs:comment "Proprietà inversa di ha valore osservato che unisce un valore osservato alla relativa osservazione."@it ;
                      
                      owl:versionInfo "stabile"@it ;
                      
                      rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                      
                      rdfs:range :Observation ;
                      
                      rdfs:domain :ObservationValue ;
                      
                      owl:inverseOf :hasObservationValue .



###  http://dati.gov.it/onto/iotapit/isPartOf

:isPartOf rdf:type owl:ObjectProperty ;
          
          rdfs:label "is part of"@en ,
                     "è parte di"@it ;
          
          rdfs:comment "Inverse property of consistsOf that links an observation to its related collection."@en ;
          
          owl:versionInfo "stable"@en ;
          
          rdfs:comment "Proprietà inversa di consiste di che lega un'osservazione alla sua relativa collezione."@it ;
          
          owl:versionInfo "stabile"@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
          
          rdfs:domain :Observation ;
          
          rdfs:range :ObservationCollection ;
          
          owl:inverseOf :consistsOf .



###  http://dati.gov.it/onto/iotapit/isValueForObservationParameter

:isValueForObservationParameter rdf:type owl:ObjectProperty ;
                                
                                rdfs:label "is value for observation parameter"@en ,
                                           "è valore per parametro osservazione"@it ;
                                
                                rdfs:comment "Inverse property of has observation value that links an observation value to the  observation parameter"@en ;
                                
                                owl:versionInfo "stable"@en ;
                                
                                rdfs:comment "Proprietà inversa di ha valore dell'osservazione che lega un valore dell'osservazione al parametro dell'osservazione."@it ;
                                
                                owl:versionInfo "stabile"@it ;
                                
                                rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                                
                                rdfs:range :ObservationParameter ;
                                
                                rdfs:domain :ObservationValue ;
                                
                                owl:inverseOf :hasValueForObservationParameter .



###  http://dati.gov.it/onto/iotapit/makesObservation

:makesObservation rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "makes observation"@en ,
                             "fa osservazione"@it ;
                  
                  rdfs:comment "Property that links a Sensor to the Observation."@en ;
                  
                  owl:versionInfo "stable"@en ;
                  
                  rdfs:comment "Proprietà che collega un sensore all'osservazione fatta dal sensore."@it ;
                  
                  owl:versionInfo "stabile"@it ;
                  
                  rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                  
                  rdfs:range :Observation ;
                  
                  rdfs:domain :Sensor .



###  http://dati.gov.it/onto/iotapit/makesObservationCollection

:makesObservationCollection rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "makes observation collection"@en ,
                                       "fa collezioni di osservazioni"@it ;
                            
                            rdfs:comment "The property links a sensor to the collection of observations that it makes."@en ;
                            
                            owl:versionInfo "stable"@en ;
                            
                            rdfs:comment "Questa proprietà collega un sensore a una collezione di osservazioni che esso fa."@it ;
                            
                            owl:versionInfo "stabile"@it ;
                            
                            rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                            
                            rdfs:range :ObservationCollection ;
                            
                            rdfs:domain :Sensor .



###  http://dati.gov.it/onto/iotapit/observationCollectionMadeBySensor

:observationCollectionMadeBySensor rdf:type owl:ObjectProperty ;
                                   
                                   rdfs:label "observation collection made by sensor"@en ,
                                              "collezione di osservazioni fatte da sensore"@it ;
                                   
                                   rdfs:comment "Inverse property of makes observation collection that links a collection of observation made by a sensor."@en ;
                                   
                                   owl:versionInfo "stable"@en ;
                                   
                                   rdfs:comment "Proprietà inversa di fa collezioni di osservazioni che unisce una collezione di osservazioni a un sensore che le produce."@it ;
                                   
                                   owl:versionInfo "stabile"@it ;
                                   
                                   rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                                   
                                   rdfs:domain :ObservationCollection ;
                                   
                                   rdfs:range :Sensor ;
                                   
                                   owl:inverseOf :makesObservationCollection .



###  http://dati.gov.it/onto/iotapit/observationMadeBySensor

:observationMadeBySensor rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "observation made by sensor"@en ,
                                    "osservazione fatta dal sensore"@it ;
                         
                         rdfs:comment "This is the inverse property of makesObservation and links an Observation to the Sensor."@en ;
                         
                         owl:versionInfo "stable"@en ;
                         
                         rdfs:comment "Questa è la proprietù inversa che lega un'osservazione al sensore."@it ;
                         
                         owl:versionInfo "stabile"@it ;
                         
                         rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> ;
                         
                         rdfs:domain :Observation ;
                         
                         rdfs:range :Sensor ;
                         
                         owl:inverseOf :makesObservation .



###  http://dati.gov.it/onto/iotapit/phenomenonTime

:phenomenonTime rdf:type owl:ObjectProperty ;
                
                rdfs:domain :Observation ;
                
                rdfs:range :TimeInterval ;
                
                rdfs:subPropertyOf :atTime .





