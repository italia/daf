    </owl:AnnotationProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/startDate -->

    <owl:AnnotationProperty rdf:about="http://dati.gov.it/onto/iotapit/startDate">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/TimeInterval"/>
    </owl:AnnotationProperty>
    


