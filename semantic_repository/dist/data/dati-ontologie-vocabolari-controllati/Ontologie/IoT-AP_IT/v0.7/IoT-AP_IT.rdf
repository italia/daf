     -->

    


    <!-- http://dati.gov.it/onto/iotapit/atTime -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/atTime">
        <rdfs:label xml:lang="en">at time</rdfs:label>
        <rdfs:label xml:lang="it">al tempo</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links an observation to the time interval in which is performed.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un&apos;osservazione all&apos;intervallo temporale in cui è condotta.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/TimeInterval"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/consistsOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/consistsOf">
        <rdfs:label xml:lang="en">consists of</rdfs:label>
        <rdfs:label xml:lang="it">consiste di</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links a collection of observations with its observations.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che collega una collezione di osservazioni con le sue osservazioni.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/ObservationCollection"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/iotapit/isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/generationTime -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/generationTime">
        <rdfs:label xml:lang="en">generation time</rdfs:label>
        <rdfs:label xml:lang="it">tempo di generazione</rdfs:label>
        <rdfs:comment xml:lang="en">This property is a subproperty of atTime and defines the generation time of the observation.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è sottoproprietà di atTime e consente di modellare il tempo di generazione della osservazione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/TimeInterval"/>
        <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/iotapit/atTime"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasAssociatedGeometry -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasAssociatedGeometry">
        <rdfs:label xml:lang="en">has associated geometry</rdfs:label>
        <rdfs:label xml:lang="it">ha geometria associata</rdfs:label>
        <rdfs:comment xml:lang="en">The property links the Observation to its geometry that is described by a type (point, line, polygon, etc.), coordinates and a coordinate reference system.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un&apos;osservazione alla sua geometria descritta in termini di tipo (linea, punto, poligono, ecc.), coordinate geografiche e sistema di riferimento delle coordinate.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/Geometry"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasFeatureOfInterest -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasFeatureOfInterest">
        <rdfs:label xml:lang="en">has feature of interest</rdfs:label>
        <rdfs:label xml:lang="it">ha caratteristica di interesse</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links the Observation to a feature of interest.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un&apos;osservazione a una caratteristica di interesse.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/FeatureOfInterest"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/iotapit/isFeatureOfInterestFor"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasMeasurementQualityValue -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasMeasurementQualityValue">
        <rdfs:label xml:lang="en">has measurement quality value</rdfs:label>
        <rdfs:label xml:lang="it">ha valore di qualità di misurazione</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links the Measrurement Quality with its value.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che collega la Misurazione di qualità con il relativo valore.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementQuality"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementQualityValue"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasMeasurementUnit -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasMeasurementUnit">
        <rdfs:label xml:lang="en">has measurement unit</rdfs:label>
        <rdfs:label xml:lang="it">ha unità di misura</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links an ObservedValue or a MesurementCapability to its Measurement Unit</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un valore osservato o un parametro di misurazione con la sua unità di misura.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementUnit"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dati.gov.it/onto/iotapit/MeasurementQuality"/>
                    <rdf:Description rdf:about="http://dati.gov.it/onto/iotapit/ObservationValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasObservationParameter -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasObservationParameter">
        <rdfs:label xml:lang="en">has observation parameter</rdfs:label>
        <rdfs:label xml:lang="it">ha parametro dell&apos;osservazione</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links an observation to its parameter being observed.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che collega un&apos;osservazione al suo parametro osservato.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/ObservationParameter"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasObservationValue -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasObservationValue">
        <rdfs:label xml:lang="en">has observation value</rdfs:label>
        <rdfs:label xml:lang="it">ha valore dell&apos;osservazione</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links an Observation to its value.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un&apos;osservazione al suo valore.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/ObservationValue"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/iotapit/isObservationValueOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasQuality -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasQuality">
        <rdfs:label xml:lang="en">has quality</rdfs:label>
        <rdfs:label xml:lang="it">ha qualità</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links an Observation to a number of measurement capabilities.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega un&apos;osservazione a una serie di parametri di qualità.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementQuality"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/hasValueForObservationParameter -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/hasValueForObservationParameter">
        <rdfs:label xml:lang="en">This property links the Observation Parameter to the Observation Value.</rdfs:label>
        <rdfs:label xml:lang="en">has value for observation parameter</rdfs:label>
        <rdfs:label xml:lang="it">Questa proprietà connette l&apos;attributo (parametro) dell&apos;osservazione a rispettivo valore dell&apos;osservazione.</rdfs:label>
        <rdfs:label xml:lang="it">ha valore per parametro di osservazione</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/ObservationParameter"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/ObservationValue"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/iotapit/isValueForObservationParameter"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/isFeatureOfInterestFor -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/isFeatureOfInterestFor">
        <rdfs:label xml:lang="en">is feature of interest for</rdfs:label>
        <rdfs:label xml:lang="it">è caratteristica di interesse per</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of has feature of interest that links a feature of interest to an observation.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di ha caratteristica di interesse che lega una caratteristica di interesse a un&apos;osservaizione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/isMeasurementQualityValueOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/isMeasurementQualityValueOf">
        <rdfs:label xml:lang="en">is measurement quality value of</rdfs:label>
        <rdfs:label xml:lang="it">è valore di qualità di misurazione di</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of has measurement quality value that links the measurement quality value to the measurment quality.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa che lega un valore della qualità di misurazione con la qualità di misurazione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementQuality"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/MeasurementQualityValue"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/iotapit/hasMeasurementQualityValue"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/isObservationParameterFor -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/iotapit/isObservationParameterFor">
        <rdfs:label xml:lang="en">is observation parameter for</rdfs:label>
