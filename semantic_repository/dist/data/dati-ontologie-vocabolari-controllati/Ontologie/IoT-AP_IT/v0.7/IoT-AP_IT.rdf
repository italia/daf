    <!-- http://dati.gov.it/onto/iotapit/description -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/description">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <rdfs:label xml:lang="it">descrizione</rdfs:label>
        <rdfs:comment xml:lang="en">This property is equivalent to http://purl.org/dc/terms/description as included in the alignment file. It is used in order to model the description of the Things of the Italian  IoT Applicaiton Profile.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è equivalente a in http://purl.org/dc/terms/description, come indicato nel file di allineamento dell&apos;ontologia ad altre esterne del web dei dati,  ed è usata allo scopo di modellare le descrizioni delle &quot;cose&quot; del profilo applicativo italiano per IoT.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/endDate -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/endDate">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/identifier -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/identifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">identifier</rdfs:label>
        <rdfs:label xml:lang="it">Identificaivo</rdfs:label>
        <rdfs:comment xml:lang="en">This property is equivalent to http://purl.org/dc/terms/identifier as included in the alignment file, and it is used in order to model an identifier for the things of the Italian IoT Application profile.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è equivalente a http://purl.org/dc/terms/identifier, come indicato nel file di allineamento di questa ontologia ad altre del web dei dati, ed è usata al fine di modellare un identificativo per una cosa del profilo italiano applicativo per l&apos;IoT.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/issued -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/issued">
        <rdfs:label xml:lang="en">time of issuance</rdfs:label>
        <rdfs:label xml:lang="it">istante di tempo (generazione)</rdfs:label>
        <rdfs:comment xml:lang="en">This property represents the istant of time that can coincide with the generation time or the startTime of an interval. The property is redundant and can be used directly for representing the observation generation time.</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La proprietà rappresenta l&apos;istante di tempo che può coincidere con il tempo di generazione o con la data di inizio dell&apos;intervallo di tempo. E&apos; una proprietà ridondante che può essere utilizzata direttamente per poter rappresentare il tempo di generazione dell&apos;osservazione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/modified -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/modified">
        <rdfs:label xml:lang="en">update time</rdfs:label>
        <rdfs:label xml:lang="it">tempo di aggiornamento</rdfs:label>
        <rdfs:comment xml:lang="en">This property is equivalent to http://purl.org/dc/terms/modified as indicated in the alignment file and it is used in order to model the update time of an observation.</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è equivalente a http://purl.org/dc/terms/modified come indicato nel file di allineamento di questa ontologia ad altre del web dei dati ed è utilizzata per modellare direttamente il tempo di modifica di un&apos;osservazione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/prefLabel -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/prefLabel">
        <rdfs:label xml:lang="en">preferred label</rdfs:label>
        <rdfs:label xml:lang="it">etichetta principale</rdfs:label>
        <owl:versionInfo xml:lang="en">Stable</owl:versionInfo>
        <rdfs:comment xml:lang="en">This property is equivalent to http://www.w3.org/2004/02/skos/core#prefLabel as indicated in the alignment file and it is used to model the preferred label associate with the things of the Italian application profile for IoT.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà è equivalente a http://www.w3.org/2004/02/skos/core#prefLabel come indicato nel file di allineamento dell&apos;ontologia ad altre esterne del web dei dati. Essa è utilizzata per modellare l&apos;etichetta principale associata alle cose dell&apos;ontologia IoT.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/startDate -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/startDate">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://dati.gov.it/onto/iotapit/value -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/iotapit/value">
        <rdfs:label xml:lang="en">value</rdfs:label>
        <rdfs:label xml:lang="it">valore</rdfs:label>
        <rdfs:comment xml:lang="en">This property represents a value. It can be the value of the observedValue or the value of the Measurement Capability.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta un valore, il valore osservato e il valore di una capacità di misurazione</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit"/>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:DatatypeProperty>
    


