###  http://dati.gov.it/onto/iotapit/date

:date rdf:type owl:DatatypeProperty ;
      
      rdfs:range xsd:dateTime .



###  http://dati.gov.it/onto/iotapit/description

:description rdf:type owl:DatatypeProperty ;
             
             rdfs:label "description"@en ,
                        "descrizione"@it ;
             
             rdfs:comment "This property is defined in http://purl.org/dc/terms and it has been added in order to model the description of the Things of the Italian  IoT Applicaiton Profile."@en ;
             
             owl:versionInfo "stable"@en ;
             
             rdfs:comment "Questa proprietà è definita in http://purl.org/dc/terms ed è stata aggiunta allo scopo di modellare le descrizioni delle \"cose\" del profilo applicativo italiano per IoT."@it ;
             
             owl:versionInfo "stabile"@it ;
             
             rdfs:comment <http://purl.org/dc/terms> ;
             
             rdfs:range rdfs:Literal ;
             
             rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/iotapit/endDate

:endDate rdf:type owl:DatatypeProperty ;
         
         rdfs:range xsd:dateTime .



###  http://dati.gov.it/onto/iotapit/generationTime

:generationTime rdf:type owl:DatatypeProperty .



###  http://dati.gov.it/onto/iotapit/identifier

:identifier rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            
            rdfs:label "identifier"@en ,
                       "Identificaivo"@it ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Questa proprietà è definita in http://purl.org/dc/terms ed è stata aggiunta al fine di modellare un identificativo per una cosa del profilo italiano applicativo per l'IoT."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy <http://purl.org/dc/terms> ;
            
            rdfs:range rdfs:Literal ;
            
            rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/iotapit/modified

:modified rdf:type owl:DatatypeProperty ;
          
          rdfs:range xsd:dateTime ;
          
          rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/iotapit/phenomenonTime

:phenomenonTime rdf:type owl:DatatypeProperty .



###  http://dati.gov.it/onto/iotapit/prefLabel

:prefLabel rdf:type owl:DatatypeProperty .



###  http://dati.gov.it/onto/iotapit/startDate

:startDate rdf:type owl:DatatypeProperty ;
           
           rdfs:range xsd:dateTime .



###  http://dati.gov.it/onto/iotapit/value

:value rdf:type owl:DatatypeProperty ;
       
       rdfs:label "value"@en ,
                  "valore"@it ;
       
       rdfs:comment "This property represents a value. It can be the value of the observedValue or the value of the Measurement Capability."@en ;
       
       owl:versionInfo "stable"@en ;
       
       rdfs:comment "Questa proprietà rappresenta un valore, il valore osservato e il valore di una capacità di misurazione"@it ;
       
       owl:versionInfo "stabile"@it ;
       
       rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit> ;
       
       rdfs:range xsd:float .





#################################################################
#
#    Classes
#
#################################################################


###  http://dati.gov.it/onto/iotapit/Accuracy

:Accuracy rdf:type owl:Class ;
          
          rdfs:label "Accuracy"@en ,
                     "Accuratezza"@it ;
          
          rdfs:subClassOf :MeasurementQuality ;
          
          rdfs:comment "It represents the Accuracy; that is, the maximimum difference between the indicated value and the actual measurand being monitored, whilst taking into account all possible sources of error that are relevant to the application."@en ;
          
          owl:versionInfo "unstable"@en ;
          
          rdfs:comment "Questa classe rappresenta l'Accuratezza, cioè la differenza massima tra il valore indicato e il valore effettivo monitorato, tenuto conto di tutte le possibili fonti di errore rilevanti per l'applicazione."@it ;
          
          owl:versionInfo "instabile"@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit> .



###  http://dati.gov.it/onto/iotapit/FDT

:FDT rdf:type owl:Class ;
     
     rdfs:label "Traffic Flow"@en ,
                "Flusso Traffico"@it ;
     
     rdfs:subClassOf :Observation ;
     
     rdfs:comment "This class represents a specific observation on the traffic flows"@en ;
     
     owl:versionInfo "unstable"@en ;
     
     rdfs:comment "Questa classe rappresenta una specifica osservazione sui flussi di traffico"@it ;
     
     owl:versionInfo "instabile"@it ;
     
     rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> .



###  http://dati.gov.it/onto/iotapit/FeatureOfInterest

:FeatureOfInterest rdf:type owl:Class ;
                   
                   rdfs:label "Feature of Interest"@en ,
                              "Caratteristica di interesse"@it ;
                   
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :isFeatureOfInterestFor ;
                                     owl:allValuesFrom :Observation
                                   ] ;
                   
                   rdfs:comment "It represents the Feature whose Observed Attribute is being observed by a Sensor in order to get an Observed Value. For instance, if we say that the temperature of the kitchen is 28 celsius degrees, the Feature of Interest is the kitchen."@en ;
                   
                   owl:versionInfo "stable"@en ;
                   
                   rdfs:comment "Rappresenta la caratteristica di interesse il cui attributo osservato è osservato da un sensor per ottenere un valore osservato. Per esempio, se diciamo che la temperatura della cucina è di 28 gradi celsius, la caratteristica di interesse è la cucina."@it ;
                   
                   owl:versionInfo "stabile"@it ;
                   
                   rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit/> .



###  http://dati.gov.it/onto/iotapit/Frequency

:Frequency rdf:type owl:Class ;
           
           rdfs:label "Frequency"@en ,
                      "Frequenza"@en ;
           
           rdfs:subClassOf :MeasurementQuality ;
           
           rdfs:comment "It represents the Frequency; that is, the smallest possible time between one observation and the next, under the defined conditions."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa classe rappresenta la frequenza ovvero il più piccolo tempo possibile tra un'osservazione e un'altra, in determinate condizioni."@it ;
           
           owl:versionInfo "instabile"@it ;
           
           rdfs:isDefinedBy <http://dati.gov.it/onto/iotapit> .



